
{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "declaration": false,
    "emitDeclarationOnly": false,
    "outDir": "../dist",
    "types": ["node", "@testing-library/jest-dom"]
  },
  "include": [
    "testing/**/*",
    "store/test/**/*",
    "**/*"
  ]
}
